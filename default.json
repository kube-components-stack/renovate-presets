{
  "regexManagers": [
    {
      "fileMatch": ["^.*/helm-generator\\.yaml$"],
      "matchStringsStrategy": "combination",
      "matchStrings": [
        "(^|\\n)name:\\s+(?:&\\S+\\s+)?(?<depName>\\S+)",
        "(^|\\n)version:\\s+(?<currentValue>\\S+)",
        "(^|\\n)repo:\\s+(?<registryUrl>\\S+)"
      ],
      "datasourceTemplate": "helm"
    },
    {
      "fileMatch": ["^crds/kube-prometheus-stack/base/kustomization\\.yaml$"],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "/helm-charts/(?<depName>[^/]+)-(?<currentValue>[^/]+)"
      ],
      "datasourceTemplate": "helm",
      "registryUrlTemplate": "https://prometheus-community.github.io/helm-charts"
    }
  ]
}