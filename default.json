{
  "regexManagers": [
    {
      "fileMatch": ["^.*/helm-generator\\.yaml$"],
      "matchStringsStrategy": "combination",
      "matchStrings": [
        "\\nname:\\s+(?:&\\S+\\s+)?(?<depName>\\S+)",
        "\\nversion:\\s+(?<currentValue>\\S+)",
        "\\nrepo:\\s+(?<registryUrl>\\S+)"
      ],
      "datasourceTemplate": "helm"
    }
  ]
}