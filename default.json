{
  "regexManagers": [
    {
      "fileMatch": ["^.*/helm-generator\\.yaml$"],
      "matchStringsStrategy": "combination",
      "matchStrings": [
        "name:\\s+(?:&\\S+\\s+)?(?<depName>\\S+)",
        "version:\\s+(?<currentValue>\\S+)",
        "repo:\\s+(?<registryUrl>\\S+)"
      ],
      "datasourceTemplate": "helm"
    }
  ]
}